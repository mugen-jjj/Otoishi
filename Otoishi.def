
;--| 基本情報 |---------------------------------------------------------------

[Info]
Name         = "Otoishi"  ;キャラクターの名前（必要な項目なので省略不可）
DisplayName  = "Otoishi"  ;M.U.G.E.Nで実際に表示される名前（省略可能）
VersionDate  = 04,14,2002     ;キャラクターの最終更新日（変えなくてもＯＫ）
MugenVersion = 1.1     ;キャラクターが動作出来るM.U.G.E.Nの改訂版
Author       = "yuto"     ;キャラクターの作成者（必要な項目なので省略不可）
Pal.Defaults = 1,2,3,4,5,6

;-----------------------------------------------------------------------------

[Files]
Cmd     = Otoishi.cmd       ;コマンドファイルの登録
Cns     = Otoishi.cns       ;コンスタンツファイルの登録
St      = Otoishi.cns       ;ステートファイルの登録・最大１１個まで増やせます
St1     = -2state.cns
St2		= Otoishi_common.cns
                        ;（St,St0,St1,St2,St3,St4,St5,St6,St7,St8,St9）
                        ;「St」のみ必要な項目なので省略出来ません
StCommon = common1.cns  ;コモンステートファイルの登録
Sprite  = Otoishi.sff       ;複数のＰＣＸ画像をまとめたスプライトファイルの登録
Anim    = Otoishi.air       ;アニメーションファイルの登録
Sound   = Otoishi.snd       ;複数のＷＡＶＥ音声をまとめたサウンドファイルの登録

;--| カラーパレットファイル |-------------------------------------------------

;キャラクターの色を定義するファイルは最大１２個まで登録が可能です。

Pal1 = act/Otoishi01.act

